#buttons with card

{
 "facebook":{
  "attachment":{
     "type":"template",
     "payload":{
        "template_type":"generic",
        "elements":[
          {
              "title":"Please accept the privacy policy to continue",
              "image_url":"https://getterms.io/img/bg-price-1.png",
              "subtitle":"Our Privacy Policy",
              "default_action":{
                 "type":"web_url",
                 "url":"https://www.privacypolicygenerator.info/live.php?token=0gfCiRjZPjqd8HzOw2ZwyYJamuaWoKGs",
                 "webview_height_ratio":"tall"
              },
              "buttons":[
                 {
                    "type":"postback",
                    "title":"I agree"
                    "payload":"I agree"

                 },
                 {
                    "type":"postback",
                    "title":"I don't agree"
                    "payload":"I don't agree"
                 }
              ]
           }
        ]
     }
  }
 }
