    "http://127.0.0.1:8000/",
  ]
}' "https://graph.facebook.com/v2.6/me/messenger_profile?access_token=EAAJxVO9CDNQBAFHw70ScBJtLU7mOXZCM9TU63GxYeTFws6pHzz1eO5MLhRmZBjXZAoUNABUog4XEQIm9aytCQvHLrQN60cbKyDgWeQIWoEkgB1Ero18ZBTLFYY960zZAxrENGtcssylaEfZAZCjGSQ9cgovSLfuZBBwvFCxgzCTGOEZAzX3i96f17"



{
  "facebook": {
     "attachment":{
     "type":"template",
     "payload":{
            "template_type": "button",
            "text": "Okay, Please select the items to order",
              "buttons":[
                 {
                    "type":"web_url",
                    "url":"http://127.0.0.1:8000/options/",
                    "title":"View Website",
                    "webview_height_ratio": "compact",
                    "messenger_extensions": "false"
                    }
              ]
         }
      }
    }
  }

