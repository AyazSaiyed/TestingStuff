// OPENING WEBVIEW //

{
  "facebook": {
    "attachment": {
      "payload": {
        "text": "Please select the items to order",
        "buttons": [
          {
            "messenger_extensions": "false",
            "webview_height_ratio": "tall",
            "url": "https://fdc174e6.ngrok.io/options",
            "type": "web_url",
            "title": "Open Menu"
          }
        ],
        "template_type": "button"
      },
      "type": "template"
    }
  }
}




